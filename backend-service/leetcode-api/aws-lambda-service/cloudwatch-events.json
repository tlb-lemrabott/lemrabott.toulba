{
  "Rules": [
    {
      "Name": "leetcode-api-warmup-rule",
      "Description": "Keep LeetCode API Lambda warm by triggering every 5 minutes",
      "ScheduleExpression": "rate(2 minutes)",
      "State": "ENABLED",
      "Targets": [
        {
          "Id": "1",
          "Arn": "arn:aws:lambda:us-east-1:YOUR_ACCOUNT_ID:function:leetcode-api-warmup",
          "Input": "{}"
        }
      ]
    }
  ]
}
